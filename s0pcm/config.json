{
    "name": "S0PCM-reader",
    "version": "0.2",
    "slug": "s0pcm",
    "description": "S0PCM",
    "uart": true,
    "url": "https://github.com/Klumpke/hassio-addons",
    "startup": "application",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "map": [
      "share:rw"
    ],
    "options": {
      "data_path": "/share/s0pcm-reader",
      "device": "/dev/ttyACM0",
      "log_level": "warning"
    },
    "schema": {
      "log_level": "list(debug|info|warning|error|critical)?",
      "data_path": "str",
      "device": "str"
    }
}