{
    "name": "S0PCM-reader",
    "version": "0.1",
    "slug": "s0pcm",
    "description": "S0PCM",
    "auto_uart": true,
    "url": "https://github.com/Klumpke/hassio-addons",
    "startup": "application",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "map": [
      "share:rw"
    ],
    "devices": ["/dev/bus/usb:/dev/bus/usb:rwm", "/dev/mem:/dev/mem:rw"],
    "options": {
      "data_path": "/share/s0pcm-reader",
      "device": "/dev/ttyACM0"
    },
    "schema": {
      "data_path": "str",
      "device": "str"
    }
}
