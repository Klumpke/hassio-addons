{
  "name": "Spotweb",
  "version": "latest",
  "slug": "spotweb_docker",
  "description": "Spotweb add-on based on erikdevries/spotweb",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "ports": {
    "80/tcp": 8080
  },
  "map": ["share:rw"],
  "webui": "http://[HOST]:[PORT:8080]",
  "boot": "auto",
  "environment": {
    "DB_HOST": "172.30.32.1",
    "DB_PORT": "3306",
    "DB_NAME": "spotweb",
    "DB_USER": "spotweb",
    "DB_PASS": "spotweb"
  },
  "schema": {},
  "options": {},
  "image":"erikdevries/spotweb"
}