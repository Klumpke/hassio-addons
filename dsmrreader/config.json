{
  "name": "DSMR Reader",
  "version": "latest",
  "slug": "dsmr",
  "description": "DSMR Reader add-on based on xirixiz/dsmr-reader-docker",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "ports": {
    "80/tcp": 8888,
    "443/tcp": 8889
  },
  "map": ["share:rw"],
  "webui": "http://[HOST]:[PORT:8888]",
  "boot": "auto",
  "environment": {
    "DB_HOST": "dsmrdb",
    "DSMR_USER": "dsmrreader",
    "DSMR_PASSWORD": "dsmrreader",
    "DSMR_EMAIL": "root@localhost"
  },
  "schema": {},
  "options": {},
  "image":"xirixiz/dsmr-reader-docker"
}